{
    "name": "pforret/pf_pageparser",
    "description": "Simple Regex Page Parser in PHP",
    "keywords": [
        "pforret",
        "parser",
        "html",
        "web",
        "pf_pageparser"
    ],
    "homepage": "https://github.com/pforret/pf_pageparser",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Peter Forret",
            "email": "peter@forret.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1",
        "ext-curl": "*",
        "guzzlehttp/guzzle": "^6.5",
        "psr/log": "^1.1",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0",
        "sempro/phpunit-pretty-print": "^1.2"
    },
    "autoload": {
        "psr-4": {
            "Pforret\\PfPageparser\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Pforret\\PfPageparser\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit --printer 'Sempro\\PHPUnitPrettyPrinter\\PrettyPrinter' ",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    }
}
